<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="wbeck.guildwars2buddy.Fragments.AccountFragment">

    <!-- TODO: Update blank fragment layout -->

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/acount_name_frag"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="12dp"
            android:layout_marginTop="15dp"
            android:text="Account.5736" />

        <TextView
            android:id="@+id/account_world"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/acount_name_frag"
            android:layout_alignBottom="@+id/acount_name_frag"
            android:layout_marginStart="51dp"
            android:layout_toEndOf="@+id/acount_name_frag"
            android:text="world" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/acount_name_frag"
            android:layout_below="@+id/acount_name_frag"
            android:layout_marginTop="16dp"
            android:text="@string/fractal_lvl_lable" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/textView6"
            android:layout_below="@+id/textView6"
            android:layout_marginTop="15dp"
            android:text="@string/wvw_rank_lable" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/textView7"
            android:layout_below="@+id/textView7"
            android:layout_marginTop="17dp"
            android:text="@string/monthly_ap_lable" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/textView9"
            android:layout_below="@+id/textView9"
            android:layout_marginTop="18dp"
            android:text="@string/daily_ap_lable" />

        <TextView
            android:id="@+id/fractal_level"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/textView6"
            android:layout_marginStart="11dp"
            android:layout_toEndOf="@+id/acount_name_frag"
            android:text="N/A" />

        <TextView
            android:id="@+id/wvw_rank"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/textView7"
            android:layout_toEndOf="@+id/acount_name_frag"
            android:text="N/A" />

        <TextView
            android:id="@+id/monthly_ap"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/textView9"
            android:layout_alignBottom="@+id/textView9"
            android:layout_alignStart="@+id/wvw_rank"
            android:text="N/A" />

        <TextView
            android:id="@+id/daily_ap"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/fractal_level"
            android:layout_alignTop="@+id/textView10"
            android:text="N/A" />

        <ListView
            android:id="@+id/accessContentList"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_alignStart="@+id/textView10"
            android:layout_below="@+id/textView10"
            android:layout_marginTop="34dp" />

    </RelativeLayout>

</FrameLayout>
